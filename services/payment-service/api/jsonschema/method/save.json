{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "JSON Schema for save",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "code": {
      "type": "string",
      "minLength": 1
    },
    "name": {
      "type": "string",
      "minLength": 1
    },
    "description": {
      "type": "string"
    },
    "icon": {
      "type": "string",
      "format": "uri",
      "minLength": 1
    },
    "categoryId": {
      "type": "integer"
    },
    "bankId": {
      "type": "integer"
    },
    "providerId": {
      "type": "integer"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    }
  },
  "required": [
    "code",
    "name",
    "description",
    "icon",
    "categoryId",
    "bankId",
    "providerId"
  ],
  "additionalProperties": false
}
