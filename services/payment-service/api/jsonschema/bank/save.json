{
	"$schema": "http://json-schema.org/draft-07/schema",
	"title": "JSON Schema for save",
	"type": "object",
	"properties": {
		"id": {
			"type": "integer"
		},
		"name": {
			"type": "string",
			"minLength": 1
		},
		"code": {
			"type": "string",
			"minLength": 1
		},
		"isActive": {
			"type": "boolean"
		}
	},
	"required": [ "name", "code" ],
	"additionalProperties": false
}
